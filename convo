    f.chatsdk.Chat.prototype.resetUnreadCount = function() {
        0 != this.unreadCount && (this.unreadCount = 0,
        f.chatsdk.Constants.DISPATCH_EVENTS_TO_UI && this.dispatch(f.chatsdk.ChatEvents.UNREAD_COUNT_CHANGED))
