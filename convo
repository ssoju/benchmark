                    if (!$(this)[0].classList.contains("clicked")) {
                        if (!$(this)[0].classList.contains("hover")) {
                            var a = Math.floor(10 * Math.random() + 1);
                            o = a.toString(),
                            1 === o.length && (o = "0" + o)
                        }
