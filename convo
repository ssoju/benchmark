                c)
                    a = this.messagesDiskManager.processFailedMessages(a, b),
                    this.delegates.forEach(function(c) {
                        "function" == typeof c.messagesLoaderLoadedHistoryMessagesInChat && c.messagesLoaderLoadedHistoryMessagesInChat(a, b, !1, null )
                    });
