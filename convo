                setTimeout(function() {
                    v.updateNetworkName()
                }, x),
                v.tab.appContCtrl.appCtrl.okToClose = function() {
                    return y !== v.networkName || v.networkImageUploadState != v.Uploader_Idle ? {
                        promptMessage: "There are unsaved change. Continue?"
                    } : void 0
                }
                ,
                r()
            }
            ]
        }
