        function c() {
            return {
                fromFilter: null ,
                toFilter: null ,
                hasFilter: null ,
                groupFilter: null ,
                userFilter: null 
            }
        }
        function d() {
            if (g != e.ONBOARDING) {
                g = e.STATE_DEFAULT,
                h = c();
                var d = a.filterInfo;
                if (!d.filterApplied || !d.filterLink.filter)
                    return void f.$broadcast("homeViewStateChange");
