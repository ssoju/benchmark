            },
            template: "<a bo-href=\"\" ng-click=\"onLikeClick()\" bo-class=\"{'like_btn': true, 'like_btn_disabled': like_pending}\" bo-bind=\"like_info.liked_by_me && 'Unlike' || 'Like'\"></a>",
            controller: ["$scope", "$rootScope", function(a, b) {
                function d() {}
                a.like_pending = !1,
                a.__leak_like = new d,
                a.onLikeClick = function() {
