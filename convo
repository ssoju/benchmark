                function m() {
                    C.draggable({
                        start: function(a, b) {
                            b.position
                        },
                        drag: function(a, b) {
                            var c = b.position;
                            c.left >= 50 && (b.position.left = 50),
                            c.top >= 50 && (b.position.top = 50),
                            x * u - -1 * b.position.left < 250 && (b.position.left = -(x * u - 250)),
                            y * u - -1 * b.position.top < 250 && (b.position.top = -(y * u - 250))
                        }
                    })
                }
