            var a = this;
            this.xmppCallsManager.subscribe(this.node, function(b) {
                a.onSubscribeResult(b)
            }, function(a) {
                this.onSubscribeError(a)
            })
        }
    }
    ,
    f.rtc.SubscribeNode.prototype.onSubscribeResult = function(a) {
