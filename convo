                    a.sort(function(a, b) {
                        var c = a.title
                          , d = b.title;
                        if (c && d) {
                            if (c = c.toLowerCase(),
                            d = d.toLowerCase(),
                            d > c)
                                return -1;
                            if (c > d)
                                return 1
                        }
                        return 0
                    })
                }
                function m(a) {
                    a.sort(function(a, b) {
                        return a.creation_date < b.creation_date ? 1 : a.creation_date > b.creation_date ? -1 : 0
                    })
                }
                function n() {
                    a.groups = [];
