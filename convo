    Strophe.addConnectionPlugin("connectionmanager", {
        pingTime: 2e4,
        timeoutTime: 1e4,
        pingInterval: null ,
        connection: null ,
        totalRetries: 0,
        maxRetries: 2,
        pingInProgress: !1,
        init: function(a) {
            this.connection = a
        },
        statusChanged: function(a) {
            var b = this;
            switch (a) {
            case Strophe.Status.CONNECTED:
            case Strophe.Status.ATTACHED:
                clearInterval(this.pingInterval),
                this.pingInterval = setInterval(function() {
                    b.pingServer()
                }, this.pingTime);
                break;
            case Strophe.Status.DISCONNECTED:
                clearInterval(this.pingInterval)
            }
        },
        pingServer: function() {
            this.pingInProgress && this.disconnect();
            var a = this
              , b = $iq({
                id: "ping",
                type: "get",
                to: this.connection.domain
            }).c("ping", {
                xmlns: "urn:xmpp:ping"
            });
            this.connection.sendIQ(b, function() {
                a.callback()
            }, function() {
                a.requestTimedOut()
            }, this.timeoutTime)
        },
        requestTimedOut: function() {
            this.pingInProgress = !1,
            this.totalRetries++,
            this.totalRetries >= this.maxRetries && this.disconnect()
        },
        disconnect: function() {
            clearInterval(this.pingInterval),
            console.log(this.maxRetries + " pings failed, disconnecting from xmpp now"),
            this.connection.disconnect()
        },
        callback: function() {
            this.pingInProgress = !1,
            this.totalRetries = 0
        }
    });
