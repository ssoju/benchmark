        this.addFetchCall = function(a, b, c, d) {
            this.sendFetchCallToServerForChat(d, a, b);
            var e = new f.chatsdk.MessagesFetchCall(c,a,b)
              , g = this.chatIdToCallsMap[d.chatId];
            g || (this.chatIdToCallsMap[d.chatId] = g = []),
            g.push(e)
        }
