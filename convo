                    var b = []
                      , d = a.files
                      , e = c;
                    for (var f in d)
                        (d[f].status === e.FILE_STATUS_CONVERSION_COMPLETED || d[f].status === e.FILE_STATUS_CONVERSION_FAILED || d[f].status === e.FILE_STATUS_CONVERSION_REQ_FAILED) && b.push(d[f]);
                    return b
                }
                ,
                k.ctrl.getAllFiles = function() {
                    return a.files
                }
                ,
