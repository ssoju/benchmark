            if (l >= q && 5 * q >= l) {
                var m = new Date(b);
                k = t[m.getDay()]
            } else if (l >= q) {
                var n = new Date(b)
                  , o = new Date(e);
                k = f ? n.getYear() == o.getYear() ? a(b, j ? j : "MMM d") : a(b, j ? j : "MMM d, yyyy") : n.getYear() == o.getYear() ? a(b, j ? j : "h:mm a, MMM d") : a(b, j ? j : "h:mm a, MMM d, yyyy"),
                h && (k = i ? k : " on " + k)
