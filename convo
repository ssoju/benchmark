        this.loadMessagesInChat = function(a, b, c, d, e) {
            var f = s.generateUniqueId();
            b = Math.max(1, b);
            var g;
            g = d ? {
                from_sequence_number: b,
                to_sequence_number: c
            } : {
                from_index: 1,
                to_index: this.MESSAGE_FETCH_PAGE_SIZE
            };
            var h = $iq({
                to: t.getConvoChatJid(),
