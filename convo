                    case l.EVENT_UPLOAD_PROGRESS:
                        a.file.eventHandlers.EVENT_UPLOAD_PROGRESS();
                        break;
                    case l.EVENT_UPLOAD_COMPLETE:
                        a.file.eventHandlers.EVENT_UPLOAD_COMPLETE();
                        break;
