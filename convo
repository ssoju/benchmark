                c.getIntegrationStatusText = function(a) {
                    for (var b = 0; b < c.configuredIntegrations.length; b++)
                        if (c.configuredIntegrations[b].webhook_id == a)
                            return "INACTIVE" == c.configuredIntegrations[b].status ? "Enable" : "Disable";
                    return "Disable"
                }
