        !1) : this.isUnsubscribed ? (console.error("User has unsubscribed from node = " + sharedID),
        !1) : this.isSynchronized ? !0 : (console.error("Node " + sharedID + " is not Syncronized"),
        !1)
    }
    ,
    f.rtc.Node.prototype.onNodeSubscribed = function() {
