                    for (var c = a.split(",")[0].split(":")[1].split(";")[0], d = new Uint8Array(b.length), e = 0; e < b.length; e++)
                        d[e] = b.charCodeAt(e);
                    return new Blob([d],{
                        type: c
                    })
                }
